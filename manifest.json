{
  "manifest_version": 3,
  "name": "Hatena Keyword Remover",
  "version": "1.5.2",
  "description": "Removes Hatena Keyword Links and search keyword highlight",
  "icons": {
    "48": "icon48.png",
    "96": "icon96.png",
    "128": "icon128.png"
  },
  "action": {
    "default_icon": {
      "32": "button32.png"
    },
    "default_title": "HKR"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "myscript.js"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{c2d3d763-9e5f-4b7d-9cf9-2843f0b78933}",
      "strict_min_version": "115.0"
    },
    "gecko_android": {
      "id": "{c2d3d763-9e5f-4b7d-9cf9-2843f0b78933}",
      "strict_min_version": "126.0"
    }
  }
}
